{
    "title": "Easy access to programming symbol on AZERTY keyboard",
    "rules": [
        {
            "description": "Easy access to brace and brakets on AZERTY keyboard using key next to return key, original key accessible with control",
            "manipulators": [
                {
                    "from": {
                        "key_code": "quote",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ],
                            "optional": [
                                "shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "quote"
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "backslash",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ],
                            "optional": [
                                "shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "backslash"
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "quote",
                        "modifiers": {
                            "optional": [
                                "shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "5",
                            "modifiers": [
                                "option"
                            ]
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "backslash",
                        "modifiers": {
                            "optional": [
                                "shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "hyphen",
                            "modifiers": [
                                "option"
                            ]
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
}